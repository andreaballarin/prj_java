import java.util.Scanner;

//Scrivere un programma che legga da tastiera un numero intero e visualizzi tutti i numeri da 10 fino a quel numero.  
//Se il numero letto Ã¨ minore di 10, non va visualizzato nulla.
public class Esercizio22103 {
	public static void main(String[] args) {
		do(
		System.out.println("Inserisci un numero");
		Scanner c = new Scanner(System.in);
		int numero= c.nextInt();
		for(int i=10; i<=numero;i++)
			System.out.println("i);
		int sum=0;
		int pro=1;
		for(int i=1; i<10; i++) {
			if(i%2==0) {
			sum=sum+i;
			pro*=i;}
	}
		System.out.println("La somma: "+sum);
			System.out.println("Il prodotto: "+pro);
			)while(numero!=10)
	}

}
